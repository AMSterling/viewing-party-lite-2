<h1>Viewing Party<h1>

<%= button_to 'Create New User', new_user_path, method: :get %>

<section id="existing_users"><p>Existing Users</p>
  <% @users.each do |user| %>
    <div id="user-<%= user.id %>">
      <%= link_to "#{user.email}'s Dashboard", user_dashboard_path(user) %>
    </div>
  <% end %>
</section>
